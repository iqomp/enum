{
    "name": "iqomp/enum",
    "description": "Static enum provider",
    "type": "library",
    "license": "MIT",
    "version": "1.1.0",
    "authors": [
        {
            "name": "Iqbal Fauzi",
            "email": "iqbalfawz@gmail.com"
        }
    ],
    "require": {
        "iqomp/config": "^1.1"
    },
    "extra": {
        "iqomp/config": "iqomp/config/",
        "iqomp/locale": "iqomp/locale/"
    },
    "autoload": {
        "psr-4": {
            "Iqomp\\Enum\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "squizlabs/php_codesniffer": "3.*"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit tests",
        "lint": "./vendor/bin/phpcs --standard=PSR12 src"
    },
    "suggest": {
        "iqomp/locale": "Allow enum to be translated to id-ID or en-US"
    }
}
