{
    "name": "iqomp/enum",
    "description": "Static enum provider",
    "type": "library",
    "license": "MIT",
    "version": "2.0.3",
    "authors": [
        {
            "name": "Iqbal Fauzi",
            "email": "iqbalfawz@gmail.com"
        }
    ],
    "require": {},
    "extra": {
        "hyperf": {
            "config": "Iqomp\\Enum\\ConfigProvider"
        }
    },
    "autoload": {
        "psr-4": {
            "Iqomp\\Enum\\": "src/"
        }
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "3.*"
    },
    "scripts": {
        "lint": "./vendor/bin/phpcs --standard=PSR12 src"
    },
    "suggest": {
        "hyperf/translation": "Allow enum to be translated to id-ID or en-US",
        "iqomp/validator": "Form validation for enum rule",
        "iqomp/formatter": "Object formatter for format type enum and multiple-enum"
    }
}
